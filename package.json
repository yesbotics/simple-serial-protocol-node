{
  "name": "simple-serial-protocol-node",
  "version": "1.0.1",
  "description": "Library for communication with arduino",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "example": "nodemon ./dist/example.js",
    "compile": "rimraf ./dist && npm run tsc",
    "dev": "nodemon -e ts  --exec \"npm run compile\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently --kill-others \"npm run tsc:w\" \"npm run example\" ",
    "list-serial-ports:linux": "dmesg | grep tty",
    "prepublish": "npm run compile",
    "tsc": "tsc --rootDir \"./src\" --outDir \"./dist\"",
    "tsc:w": "tsc -w --rootDir \"./src\" --outDir \"./dist\" --inlineSourceMap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/yesbotics/simple-serial-protocol/simple-serial-protocol-node/tree/legacy/einspunktnull/1.x"
  },
  "keywords": [
    "node",
    "nodejs",
    "node.js",
    "arduino",
    "serial",
    "protocol"
  ],
  "author": "Albrecht Nitsche",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/yesbotics/simple-serial-protocol/simple-serial-protocol-node/issues"
  },
  "homepage": "https://gitlab.com/yesbotics/simple-serial-protocol/simple-serial-protocol-node#readme",
  "dependencies": {
    "serialport": "6.0.0",
    "typescript-collections": "1.2.3"
  },
  "devDependencies": {
    "@types/serialport": "4.0.9",
    "concurrently": "3.5.0",
    "nodemon": "1.11.0",
    "rimraf": "3.0.0",
    "typescript": "2.5.1"
  }
}
